${app}:
  image: java:8
  working_dir: /app
  net: host
  volumes:
    - ./app:/app

  command: java -Xmx${jmx} -Xss256k -jar /app/${app}-${ver}.jar --spring.profiles.active=${profile}

  ports:
${portsArray}

